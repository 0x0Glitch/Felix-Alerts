{
  "database": {
    "market_data_schema": "market_data",
    "user_positions_schema": "user_positions",
    "market_data_table": "flx_tsla_data",
    "positions_table": "flx_tsla_positions"
  },
  "api": {
    "dex": "flx"
  },
  "symbols": {
    "primary_coin": "flx:TSLA",
    "allowed_liquidation_coins": ["flx:TSLA"]
  },
  "deviation_oracle_price": {
    "check_interval_seconds": 3,
    "threshold_percent": 0.5
  },
  "funding_rate": {
    "check_interval_seconds": 360,
    "threshold": 25
  },
  "oi_oicap": {
    "check_interval_seconds": 1080,
    "threshold_percent": 0.85
  },
  "depth": {
    "check_interval_seconds": 300,
    "threshold_percent": 0.2
  },
  "impact_price_difference": {
    "check_interval_seconds": 120,
    "threshold_percent": 3
  },
  "liquidation_alert": {
    "check_interval_seconds": 3000,
    "min_position_value": 50000,
    "max_leverage": 5,
    "margin_threshold_multiplier": 1.10
  },
  "big_position": {
    "threshold": 10000,
    "ping_interval_seconds": 21,
    "reconnect_base_delay": 1.0,
    "reconnect_max_delay": 15.0,
    "max_trade_age_seconds": 20
  },
  "big_liquidation": {
    "usd_threshold": 50000,
    "read_existing_at_start": false,
    "read_new_files_from_start": true
  },
  "stale_oracle_alerts": {
    "threshold_seconds": 1.0,
    "read_existing_at_start": false,
    "read_new_files_from_start": true
  }
}
